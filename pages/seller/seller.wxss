/* pages/seller/seller.wxss */
.tdzy{
  border-top: 1px solid #F3F3F3;
  border-bottom: 20rpx solid #F5F5F5;
  text-align: center;
}
.tdzy image{
  width: 138rpx;
  height: 138rpx;
  border-radius: 100%;
  margin: 40rpx 0 16rpx;
}
.tdzy>text{
  font-size: 32rpx;
  color: #333;
  display: block;
}
.tdzy>view{
  display: flex;
  justify-content: space-between;
  margin: 38rpx 0 0;
  border-top: 1px solid #EEEEEE;
}
.tdzy>view view{
  width: 33.3%;
  box-sizing: border-box;
  border-right: 1px solid #ECECEC;
  margin: 30rpx 0;
  font-size: 34rpx;
  color: #222;
}
.tdzy>view view:last-child{
  border-right: 0;
}
.tdzy>view text{
  display: block;
  color: #999;
  font-size: 26rpx;
}
.nr{
  font-size: 28rpx;
  color: #444;
  line-height: 46rpx;
}
 .nr_img{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin: 10rpx 0;
}
.nr_img view{
  width: 32%;
  margin-right:1.5%; 
}
.nr_img view image{
  width: 100%;
}
.nr_img view:nth-child(3n){
  margin-right: 0;
} 
.times{
  font-size: 24rpx;
  color: #999;
  margin: 10rpx 0;
}
.times image{
  width: 30rpx;
  margin-right: 10rpx;
}
.times text{
  margin-right: 20rpx;
  display: inline-block;
}
.address{
  font-size: 24rpx;
  color: #999;
  margin-top: 20rpx;
}
.address image{
  width: 15rpx;
  margin-right: 10rpx;
}
.product{
  width: 94%;
  margin: 0 auto;
}
.product>view{
  border-bottom: 1px solid #E1E1E1;
  padding: 10rpx 0;
}
.gz{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: #fff;
  text-align: center;
  z-index: 10000;
}
.gz navigator{
  width: 20%;
}
.gz navigator image{
  width: 35rpx;
  margin-top: 20rpx;
}
.gz navigator text{
  color: #606060;
  font-size: 20rpx;
  display: block;
}
.gz .rt{
  background: #333333;
  line-height: 100rpx;
  color: #fff;
  font-size: 30rpx;
  width: 80%;
}



