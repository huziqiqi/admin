page {
  background: #f4f4f4;
}

.wrap {
  background: #fff;
}

.list {
  padding: 0 20rpx;
  margin-top: 20rpx;
}

.list-item {
  padding: 30rpx 20rpx;
  border-bottom: 1px solid #f5f5f5;
  justify-content: space-between;
  align-items: center;
}

.list-item>text {
  font-size: 28rpx;
  color: #333;
  flex: 1;
}

.list-item image {
  width: 20rpx;
  height: 20rpx;
}