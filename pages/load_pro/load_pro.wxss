/* pages/load_pro/load_pro.wxss */
.upload {
  font-size: 30rpx;
  line-height: 80rpx;
  height: 80rpx;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
  color: #333;
  text-align: center;
  background: #222222;
}

.upload .lf {
  color: #fbda00;
}

.upload .rt {
  background: #fff;
}

.upload image {
  width: 6.5%;
}

.upload view {
  width: 47%;
}

.fileList {
  background: #F5F5F5;
  margin-top: 10rpx;

  /* text-align: center; */
}

.fileList image {
  width: 100%;
}

.list {
  font-size: 28rpx;
  padding: 0 20rpx;
  color: #444444;
  line-height: 100rpx;
  border-bottom: 20rpx solid #F5F5F5
}

.list .lf {
  display: inline-block;
  vertical-align: middle;
  width: 25%;
}

.list .clear input.rt {
  height: 50rpx;
  margin-top: 25rpx;
}

.list .clear>.rt {
  width: 72%;
}

.list .clear {
  border-bottom: 1px solid #EAEAEA;
}

.list .clear:last-child {
  border: 0;
}

.list image {
  width: 15rpx;
  margin-top: 40rpx;
}

.next {
  width: 94%;
  margin: 40rpx auto;
  border-radius: 5px;
  background: #222222;
  color: #fcd903;
  font-size: 28rpx;
  line-height: 90rpx;
  text-align: center;
}


















/* pages/entrust/entrust.wxss */
.article {
  padding: 0;
  background: #f6f7f8;
}

.title {
  display: block;
  font-size: 16px;
  color: #666;
  padding: 30rpx;

}

.item {
  background: #FFF;
  padding: 0 30rpx;

}

.item>view {
  justify-content: left;
  font-size: 14px;
  border-bottom: 1px solid #e0efea;
}

.item>view.flex>text {
  display: block;
  height: 44rpx;
}

.item>view.flex>text:nth-of-type(1) {
  flex: 0 0 60px;
  text-align: justify;
  height: 38rpx;
  overflow: hidden;
}

.item>view>text:nth-of-type(1)::after {
  display: inline-block;
  width: 100%;
  content: "";
}

.item>view:last-of-type {
  border-bottom: none;
}

.item>view .btn {
  flex: 1;
}

.item>view .btn view {
  display: inline-block;
  line-height: 60rpx;
  margin: 10rpx 0;
  width: 150rpx;
  border: 1px solid #333;
  color: #333;
  background: #f5f5f5;
  text-align: center;
  margin-left: 40rpx;
  border-radius: 30rpx;
}

.item>view .text {
  flex: 1;
}

.item>view .text input {
  width: 300rpx;
}

.item>view picker {
  margin-left: 40rpx;
  flex: 1;
}

.item>view .btn .cur {
  color: #FFF;
  background: #009966;
  border-color: #009966;
}

.item>view input {
  display: inline-block;
  vertical-align: middle;
  width: 284rpx;
  color: #666;
  height: 90rpx;
  margin-left: 40rpx;
  flex: 1;
}

.item>view .value {
  color: #666;
  line-height: 90rpx;
  margin-left: 0;
}

.item>view .company {
  display: inline-block;
  vertical-align: middle;
}

.item>view .icomoon {
  color: #aff;
}

.item>view .box>text {
  display: block;
  color: #ff1365;
  background: #aaa;
  font-size: 11px;
  margin-top: 20rpx;
}

.item>view .box .fileList {
  display: inline-block;
  width: 150rpx;
  text-align: center;
}

.item>view .box .fileList .file {
  width: 150rpx;
  position: relative;
}

.item>view .box .fileList .file>view {
  border: 1px dashed #f5f5f5;
  height: 150rpx;
  position: relative;
  z-index: 2;
}

.item>view .box .fileList .file text {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  font-size: 30px;
  margin: 0;
  color: #AAA;
}

.imgbox {
  width: 110rpx;
  height: 110rpx;
  padding: 20rpx;
  margin: 0 0 30rpx 30rpx;
  border: 1px dashed #f5f5f5;
  position: relative;
  overflow: hidden;
}

.imgbox image {
  display: block;
  width: calc(100% - 40rpx);
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.imgbox:nth-of-type(3n-2) {
  margin-left: 0;
}

button {
  display: block;
  margin: 60rpx 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #FFF;
  background: #009966;
  border-radius: 50rpx;
}

.lable {
  font-size: 0;
}

.lable label {
  display: inline-block;
  font-size: 28rpx;
  width: 150rpx;
  line-height: 60rpx;
  text-align: center;
  margin: 20rpx 20rpx 0 0;
  background: #f5f5f5;
}

.lable label.cur {
  background: #009966;
  color: #FFF;
}